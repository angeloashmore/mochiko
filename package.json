{
  "name": "mochiko",
  "version": "0.3.0",
  "description": "An automated GitHub issues generator for new projects",
  "main": "build/index.js",
  "module": "src/index.js",
  "bin": {
    "mochiko": "build/index.js"
  },
  "scripts": {
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "build": "babel src --out-dir build",
    "watch": "npm run build -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/mochiko",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/mochiko.git"
  },
  "keywords": [
    "github",
    "management"
  ],
  "author": "Angelo Ashmore <angeloashmore@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "colors": "^1.1.2",
    "gray-matter": "^3.0.2",
    "node-fetch": "^1.7.1",
    "yargs": "^8.0.2"
  }
}
